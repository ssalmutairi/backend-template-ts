{
  "name": "backend-template-ts",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && c8 --reporter=html node --test -r ts-node/register test/**/*.ts",
    "start": "fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "prepare": "husky",
    "prisma": "npx prisma generate"
  },
  "keywords": [],
  "author": "autherName <email@email.com>",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^6.0.2",
    "@fastify/compress": "^8.0.1",
    "@fastify/cookie": "^11.0.1",
    "@fastify/cors": "^10.0.1",
    "@fastify/formbody": "^8.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/redis": "^7.0.1",
    "@fastify/sensible": "^6.0.1",
    "@fastify/swagger": "^9.1.0",
    "@fastify/swagger-ui": "^5.0.1",
    "@fastify/type-provider-typebox": "^5.0.0",
    "@fastify/websocket": "^11.0.1",
    "@prisma/client": "^5.20.0",
    "@sinclair/typebox": "^0.33.15",
    "bcrypt": "^5.1.1",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.0.1",
    "fastify-plugin": "^5.0.1",
    "husky": "^9.1.6",
    "i18next": "^23.15.2",
    "i18next-fs-backend": "^2.3.2",
    "i18next-http-middleware": "^3.6.0",
    "moment": "^2.30.1",
    "prisma": "^5.20.0",
    "qs": "^6.13.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@platformatic/client": "^2.5.3",
    "@types/bcrypt": "^5.0.2",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.7.5",
    "@types/tap": "^15.0.12",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "c8": "^10.1.2",
    "concurrently": "^9.0.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fastify-tsconfig": "^2.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  }
}
